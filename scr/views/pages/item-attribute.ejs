<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <table>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>preço</th>
            <th>categoria</th>
            <th>reputação</th>
        </tr>
    <% itemdata.forEach(function(itemdata){ %>
        <tr>
            <td><%- itemdata.id %>&nbsp;&nbsp;&nbsp;</td>
            <td><a href="<%- itemdata.permalink %>"><%- itemdata.title %></a>&nbsp;&nbsp;&nbsp;</td>
            <td><%- itemdata.price %>&nbsp;&nbsp;&nbsp;</td>
            <td><%- itemdata.category_id %>&nbsp;&nbsp;&nbsp;</td>
            <td><%- itemdata.listing_type__id %>&nbsp;&nbsp;&nbsp;</td>
          </tr>
      <% }); %>
    </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>