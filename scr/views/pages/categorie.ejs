<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <table>
        <tr>
            <th>id</th>
            <th>name</th>
            <th></th>
            <th></th>
        </tr>
    <% data.forEach(function(data){ %>
        <tr>
            <td><%- data.id %></td>
            <td><a href="/categories/<%- data.id %>"><%- data.name %></a></td>
            <td><a href="/categories/<%- data.id %>/trends">Tendencias</a></td>
            <td><a href="/categories/<%- data.id %>/items">Items</a></td>
            <td><a href="/categories/<%- data.id %>/20mais">Items mais vendidos</a></td>
          </tr>
      <% }); %>
    </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>