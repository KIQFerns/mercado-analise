<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <table>
    <tr>
        <th>id</th>
        <th>name</th>
        <th>Volume de itens</th>
    </tr>
    <% for(var item in datacategorie.children_categories){%>
        <tr>
            <td>
                <%= datacategorie.children_categories[item].id%>
            </td>
            <td>
                <%= datacategorie.children_categories[item].name%>
            </td>
            <td>
                <%= datacategorie.children_categories[item].total_items_in_this_category%>
            </td>
            <td><a href="/trends/<%= datacategorie.children_categories[item].id%>/trends">Tendencias</a>&nbsp;&nbsp;&nbsp;</td>
            <td><a href="/api/<%= datacategorie.children_categories[item].id%>/items">Items</a>&nbsp;&nbsp;&nbsp;</td>
            <td><a href="/highlights/<%= datacategorie.children_categories[item].id%>/20mais">Mais Vendidos</a>&nbsp;&nbsp;&nbsp;</td>
        </tr>
        <% } %>
    </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>