<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <h2>Termos mais buscados</h2>
    <ol class="list-group">
    <% datatrends.forEach(function(datatrends){ %>
            <li 
                class="list-group-item" 
                style="display: list-item;
                        padding: 0.25rem 1.25rem;"><a href="<%- datatrends.url %>"><%- datatrends.keyword %></a></li>
      <% }); %>
    </ol>

</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>