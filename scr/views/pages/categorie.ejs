<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <h2>sites</h2>
    <table>
        <tr>
            <th>id</th>
            <th>name</th>
        </tr>
    <% data.forEach(function(data){ %>
        <tr>
            <td><%- data.id %></td>
            <td><%- data.name %></td>
          </tr>
      <% }); %>
    </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>