<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <table class="table">
        <tr>
            <th scope="col">id</th>  
            <th scope="col">title</th>
            <th scope="col">vendedor</th> 
            <th scope="col">price</th>
            <th scope="col">unidades vendidas</th>      
        </tr>
        <% for(var item in datasearch.results){%>
            <tr>
                <td>
                    <%= datasearch.results[item].id%>
                </td>
                <td>
                    <%= datasearch.results[item].title%>
                </td>
                <td>
                    <%= datasearch.results[item].seller.id%>
                </td>
                <td>
                    <%= datasearch.results[item].price%>
                </td>
                <td>
                    <%= datasearch.results[item].sold_quantity%>
                </td>
            </tr>
            <% } %>
        </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>