<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Sites</h1>
    <table>
        <tr>
            <th>default_currency_id</th>
            <th>id</th>
            <th>name</th>
        </tr>
    <% data.forEach(function(data){ %>
        <tr>
            <td><%- data.default_currency_id %></td>
            <td><%- data.id %></td>
            <td><%- data.name %></td>
          </tr>
      <% }); %>
    </table>
</body>
</html>