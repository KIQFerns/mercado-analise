<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
    <%- include('../partials/header'); %>
</header>

<main>
    <table class="table">
    <tr>
        <th scope="col">posição</th>
        <th scope="col">id</th>
        <th scope="col">tipo</th>
        <th scope="col">posição</th>
    </tr>
    <% for(var item in datamais.content){%>
        <tr>
            <td>
                <%= datamais.content[item].position%>
            </td>
            <td>
                <%= datamais.content[item].id%>
            </td>
            <td>
                <a href="<% if (datamais.content[item].type == 'ITEM') { %>
                    /items/<%= datamais.content[item].id%>
                <% } else { %>
                    /products/<%= datamais.content[item].id%>
                <% } %>
                    ">
                    <%= datamais.content[item].type%>
                </a>
            </td>
            <td>
                <% if (datamais.content[item].type == 'ITEM') { %>
                    <a href="highlights/item/<%= datamais.content[item].id%>">posição</a>"
                <% } else { %>
                    <a href="highlights/product/<%= datamais.content[item].id%>">posição</a>"
                <% } %>
                    ">
            </td>
        </tr>
        <% } %>
    </table>
</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>